{
  "name": "node-cqrs",
  "version": "0.16.0-0",
  "description": "Basic ES6 backbone for CQRS app development",
  "repository": "snatalenko/node-cqrs",
  "keywords": [
    "cqrs",
    "ddd",
    "domain",
    "command",
    "event",
    "eventstore",
    "eventsourcing"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=10.3.0"
  },
  "scripts": {
    "lint": "npx eslint src/",
    "lint:strict": "npx eslint src/ --max-warnings=0",
    "pretest": "npm run lint",
    "test": "mocha ./tests/index.js",
    "test:integration": "mocha ./examples/user-domain-tests",
    "test:coverage": "nyc --reporter=lcov --reporter=text-summary node_modules/.bin/_mocha tests/index.js",
    "pretest:coveralls": "npm run test:coverage",
    "test:coveralls": "cat ./coverage/lcov.info | coveralls",
    "posttest:coveralls": "rm -rf ./coverage",
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s",
    "preversion": "npm run lint:strict && npm test",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "author": "@stas_natalenko",
  "license": "MIT",
  "homepage": "https://github.com/snatalenko/node-cqrs#readme",
  "dependencies": {
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "sinon": "^7.5.0"
  }
}
